                        <li>@Html.ActionLink("Single Product" , "SearchProduct" , "SingleProduct")</li>
                        <li>@Html.ActionLink("Cart" , "cart" , "MobileCart")</li>
                        <li>@Html.ActionLink("SignUp" , "SignUp" , "Home")</li>
                    </ul>
                </div>  
            </div>
        </div>
    </div> <!-- End mainmenu area -->
    
