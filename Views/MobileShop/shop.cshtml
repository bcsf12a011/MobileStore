                                <ul class="dropdown-menu">
                                  <li>@Html.ActionLink("AdminLogin" , "ALogin" , "AdminLogin")</li>
                                    <li>@Html.ActionLink("CustomerLogin" , "CustomerLogin" , "CustomerLogin")</li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- End header area -->
    
    <div class="site-branding-area">
